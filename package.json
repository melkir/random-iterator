{
  "name": "random-iterator",
  "version": "0.1.4",
  "description": "Iterate values in a list in random order",
  "main": "index.js",
  "scripts": {
    "test": "xo && nyc ava -v"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melkir/random-iterator.git"
  },
  "keywords": [
    "array",
    "generator",
    "iterator",
    "next"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "*",
    "codecov": "^3.0.0",
    "nyc": "^11.4.1",
    "xo": "*"
  },
  "author": {
    "name": "melkir",
    "email": "mail@thibaultvieux.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/melkir/random-iterator/issues"
  },
  "xo": {
    "space": true,
    "prettier": true,
    "rules": {
      "eqeqeq": [
        "error",
        "smart"
      ],
      "no-eq-null": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSpacing": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/melkir/random-iterator#readme"
}
